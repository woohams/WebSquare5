    			<xf:trigger type="button" id="btn_insertRow" style="width:130px;" class="dmr btnDef"
    				ev:onclick="scwin.btn_insertRow_onclick">
    				<xf:label><![CDATA[insertRow]]></xf:label>
    			</xf:trigger>
    			<xf:trigger class="dmr btnDef" ev:onclick="scwin.btn_insertJSON_onclick" id="btn_insertJSON" style="width:130px;"
    				type="button">
    				<xf:label><![CDATA[insertJSON]]></xf:label>
    			</xf:trigger><xf:trigger class="dmr btnDef" ev:onclick="scwin.btn_insertRowBroadCast_onclick" id="btn_insertRowBroadCast" style="width:160px;" type="button">
    				<xf:label><![CDATA[insertRow(BroadCast)]]></xf:label>
    			</xf:trigger>

    			<xf:trigger class="dmr btnLightGray" ev:onclick="scwin.btn_removeRow_onclick" id="btn_removeRow" style="width:130px;"
    				type="button">
    				<xf:label><![CDATA[removeRow]]></xf:label>
    			</xf:trigger>
    			<xf:trigger class="dmr btnLightGray" ev:onclick="scwin.btn_removeRows_onclick" id="btn_removeRows" style="width:130px;"
    				type="button">
