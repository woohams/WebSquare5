	scwin.ui_grid_EMP_NM = function(data, formattedData, rowIndex, colIndex){
		if(formattedData.indexOf("영") > 0){
			ui_grid.setCellColor( rowIndex , colIndex , "red" );
		}
		return formattedData;
	};
	
	scwin.ui_grid_GENDER_CD = function(data, formattedData, rowIndex, colIndex){
		if(data === "F"){
			ui_grid.setCellColor( rowIndex , colIndex , "orange" );
		}
		return formattedData;
	};
	
	scwin.ui_grid_ROLE_CD = function(data, formattedData, rowIndex, colIndex){
		if(data === "02"){
			ui_grid.setCellColor( rowIndex , colIndex , "blue" );
		}
		return formattedData;
	};
	
	scwin.ui_grid_POSITION_CD = function(data, formattedData, rowIndex, colIndex){
		if(data === "07"){
			ui_grid.setCellBackgroundColor( rowIndex , colIndex , "blue" );
			ui_grid.setCellColor( rowIndex , colIndex , "#fff" );
		}
		return formattedData;
	}; 
	
	scwin.ui_grid2_EMP_NM = function(data, formattedData, rowIndex, colIndex){
		if(data.indexOf("영") > 0){
			return "<span style='color:red'>"+formattedData+"</span>";
		}
		return data;
	};
	
	scwin.ui_grid2_GENDER_CD = function(data, formattedData, rowIndex, colIndex){
		if(data === "F"){
			return "<span style='color:orange'>"+formattedData+"</span>";
		}
		return formattedData;
	};
	
	scwin.ui_grid2_ROLE_CD = function(data, formattedData){
		if(data === "02"){
			return "<span style='color:blue'>"+formattedData+"</span>";
		}
		return formattedData;
	};
	
	scwin.ui_grid2_POSITION_CD = function(data, formattedData, rowIndex, colIndex){
		if(data === "07"){
			return "<div style='background-color:blue;line-height:24px;color:#fff;'>"+formattedData+"</div>";
		}
		return formattedData;
	};]]></script>
