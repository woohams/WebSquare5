	};
	
	
	
	scwin.ui_search_onclick = function(e) {
		scwin.$w.executeSubmission( "sbm_userInfo" );
	};
	]]></script>
    </head>
    <body ev:onpageload="scwin.onpageload" ev:onpageunload="scwin.onpageunload">
    	<xf:group class="" id="" style="padding: 10px;">
