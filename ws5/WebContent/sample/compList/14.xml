		<script type="javascript" lazy="false"><![CDATA[

	//차트 옵션 적용
	scwin.fn_setChartOpt = function() {
		var options = {
			startValue : "20000",
			color : "FF0030",
			displayValue : "TrendLine"
		};
		ui_fusionchart1.addTrendLine(options);

		ui_fusionchart1.setChartAttribute({
			Caption : "Quarter2 Data"
		});
		var options1 = {
			color : "20B2AA",
			font : "15",
			bold : "1"
		};
		ui_fusionchart1.applyStyle("font", "captionFont", options1, ["Caption",
				"YAXISNAME"]);
		ui_fusionchart1.draw();
	};

	//selectBox list 추가
	scwin.fn_setSelectBox = function() {
		var select1Data = ["column2d", "column3d", "bar2d", "bar3d", "area2d",
				"line", "pareto2d", "pareto3d", "pie2d", "pie3d", "doughnut2d",
				"doughnut3d"];
		var select2Data = ["MSColumn2D", "MSColumn3D", "MSLine", "ZoomLine",
				"MSArea", "MSBar2D", "MSBar3D", "Marimekko", "StackedColumn2D",
				"StackedColumn3D", "StackedArea2D", "StackedBar2D",
				"StackedBar3D"];

		scwin.fn_setItemList("ui_selectbox1", select1Data);
		scwin.fn_setItemList("ui_selectbox2", select2Data);
	};

	scwin.fn_setItemList = function(objName, arrData) {
		var tmpVal;
		var curComp = $p.getComponentById(objName); 
		for (var i = 0; i < arrData.length; i++) {
			tmpVal = arrData[i];
			curComp.addItem(tmpVal, tmpVal);
		}

	};

	scwin.ui_selectbox1_onchange = function() {
		var chartType = this.getValue();
		ui_fusionchart1.changeType(chartType);
	};

	scwin.ui_selectbox2_onchange = function() {
		var chartType = this.getValue();
		ui_fusionchart2.changeType(chartType);
	};

	scwin.ui_selectbox3_onchange = function() {
		var chartType = this.getValue();
		ui_fusionchart3.changeType(chartType);

		if (chartType == "MSCombi3D" || chartType == "MSCombi2D") {
			ui_fusionchart3
					.setChartColumnRef(
							'data:dc_chartData2',
							'Month',
							"[['quarter1',{'id':'quarter2','renderas':'Area'},{'id':'quarter3','renderas':'Line'},'quarter4']]");
		} else if (chartType == "MSColumnLine3D") {
			ui_fusionchart3
					.setChartColumnRef(
							'data:dc_chartData2',
							'Month',
							"[['quarter1','quarter2',{'id':'quarter3','renderas':'Line'},{'id':'quarter4','renderas':'Line'}]]");
		} else if (chartType == "StackedColumn2DLine"
				|| chartType == "StackedColumn3DLine") {
			ui_fusionchart3
					.setChartColumnRef(
							'data:dc_chartData2',
							'Month',
							"[['quarter1','quarter2',{'id':'quarter3','renderas':'Line'},{'id':'quarter4','renderas':'Line'}]]");
		} else if (chartType == "MSCombiDY2D"
				|| chartType == "MSColumn3DLineDY"
				|| chartType == "StackedColumn3DLineDY") {
			ui_fusionchart3
					.setChartColumnRef(
							'data:dc_chartData2',
							'Month',
							"[['quarter1','quarter2',{'id':'quarter3','renderas':'Line','parentYAxis':'S'},{'id':'quarter4','renderas':'Line','parentYAxis':'S'}]]");
		};
	};

	scwin.ui_selectbox4_onchange = function() {
		var chartType = this.getValue();
		ui_fusionchart4.changeType(chartType);
	};

	scwin.onpageload = function() {
		scwin.fn_setChartOpt();
		scwin.fn_setSelectBox();
	};

	scwin.onpageunload = function() {

	};
]]></script>
