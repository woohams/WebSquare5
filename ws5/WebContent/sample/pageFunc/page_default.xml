							</xf:group>

							<xf:trigger class="btnRed fr" ev:onclick="scwin.btn_Search_onclick" id="btn_Search" style=""
								type="button">
								<xf:label><![CDATA[조회]]></xf:label>
							</xf:trigger>
						</xf:group>
					</xf:group>
				</xf:group>
			</xf:group>
			<xf:group class="defaultbox" id="" style="">
				<xf:group class="defaultbox tar" id="" style="">
