        	</xf:submission>
        </xf:model>
        <script type="text/javascript" lazy="false"><![CDATA[
        
	scwin.onpageload = function() {
		$p.executeSubmission( "sbm_codeParent" );
	};
	
	scwin.onpageunload = function() {
		
	};
	
	
	scwin.trigger1_onclick = function(e) {
		alert( wframe_tab.getWindow( 0 ).ui_form_name.getValue() );
	};
	
	scwin.trigger2_onclick = function(e) {
		//dc_codeGender.setJSON( wframe_tab.getWindow( 0 ).dc_code101.getAllJSON() );
		//p_gender.setNodeSet( "data:dc_codeGender" , "CODE_NM" , "CODE_CD" );
		
		alert( wframe_tab.getWindow( 0 ).ui_form_gender.getValue() );
	};
	
	scwin.trigger3_onclick = function(e) {
		alert( wframe_tab.getWindow( 0 ).ui_form_date.getValue() );
	};
	
	scwin.trigger4_onclick = function(e) {
		//dc_codePosition.setJSON( wframe_tab.getWindow( 0 ).dc_code02.getAllJSON() );
		//p_position.setNodeSet( "data:dc_codePosition" , "CODE_NM" , "CODE_CD" );
		
		alert( wframe_tab.getWindow( 0 ).ui_form_position.getValue() );
	};
	
	scwin.sbm_codeParent_submit = function(e) {
		dc_reqCodeParent.set( "GRP_CD" , "02,101" );
	};
	
	scwin.sbm_codeParent_submitdone = function(e) {
		
	};
	]]></script>
